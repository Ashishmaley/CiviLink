<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/profile_edit_graph"
    app:startDestination="@id/displayProfileFragment">

    <fragment
        android:id="@+id/displayProfileFragment"
        android:name="com.example.civilink.UserProfile.DisplayProfileFragment"
        android:label="fragment_display_profile"
        tools:layout="@layout/fragment_display_profile" >
        <action
            android:id="@+id/action_displayProfileFragment_to_editProfileFragment"
            app:destination="@id/editProfileFragment" />
    </fragment>
    <fragment
        android:id="@+id/editProfileFragment"
        android:name="com.example.civilink.UserProfile.EditProfileFragment"
        android:label="fragment_edit_profile"
        tools:layout="@layout/fragment_edit_profile" />
</navigation>